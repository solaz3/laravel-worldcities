{
  "name": "coldcoder/laravel-worldcities",
  "description": "A Laravel package to retrieve cities in the world",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ou Junhui",
      "email": "solaze@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0",
    "illuminate/container": "~5.5.0|~5.6.0",
    "illuminate/contracts": "~5.5.0|~5.6.0",
    "illuminate/database": "~5.5.0|~5.6.0",
    "illuminate/support": "~5.5.0|~5.6.0",
    "spatie/laravel-translatable": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0",
    "orchestra/testbench": "~3.5.0|~3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Coldcoder\\WorldCity\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coldcoder\\WorldCity\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coldcoder\\WorldCity\\WorldCityServiceProvider"
      ]
    }
  }
}
