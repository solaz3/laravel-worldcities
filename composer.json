{
  "name": "coldcoder/laravel-worldcities",
  "description": "A Laravel package to retrieve cities in the world",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ou Junhui",
      "email": "solaze@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/container": "~5.4|~6|~7|~8|~9|~10",
    "illuminate/contracts": "~5.4|~6|~7|~8|~9|~10",
    "illuminate/database": "~5.4|~6|~7|~8|~9|~10",
    "illuminate/support": "~5.4|~6|~7|~8|~9|~10",
    "spatie/laravel-translatable": "^2.0|~3|~4|~5|~6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0|~8|~9|~10",
    "orchestra/testbench": "~3.5.0|~3.6.0|~4|~5|~6|~7"
  },
  "autoload": {
    "psr-4": {
      "Coldcoder\\WorldCity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coldcoder\\WorldCity\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Coldcoder\\WorldCity\\WorldCityServiceProvider"
      ]
    }
  }
}
